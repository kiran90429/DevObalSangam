    <script type="text/javascript">
        $(document).ready(function () {

            $(".srchimg").attr('placeholder', "Searcher (F9)");
            $(".hasDatepicker").attr('placeholder', "DD/MM/YYYY");

            // Alpanumeric values only
            $.validator.addMethod("alphanumeric", function (value, element) {
                return this.optional(element) || /^[a-z0-9\s]+$/i.test(value);
            }, "Enter only Alphanumerics");

            // Alpanumeric values with - and /
            $.validator.addMethod("alphanumericwith", function (value, element) {
                return this.optional(element) || /^[a-zA-Z0-9_\s\+\-\/]+$/.test(value);
            }, "Enter only Alphanumerics");

            // Alpanumeric values with - and /
            $.validator.addMethod("alphanumericBackslash", function (value, element) {
                return this.optional(element) || /^[a-zA-Z0-9_\s\+\/]+$/.test(value);
            }, "Enter only Alphanumerics");

            // Aphabets only with "."
            $.validator.addMethod("alphabets", function (value, element) {
                return this.optional(element) || /^[a-zA-Z\.\s]+$/.test(value);
            }, "Enter only alphabets");


            //Alphanumerics values with .,space
            $.validator.addMethod("alphabet", function (value, element) {
                return this.optional(element) || /^[a-zA-Z0-9\.\s\,\']+$/.test(value);
            }, "Enter only Alphanumerics");

            // Aphabets only 
            $.validator.addMethod("alphabetsonly", function (value, element) {
                return this.optional(element) || /^[a-zA-Z\.\s\,\']+$/.test(value);
            }, "Enter only Alphabets");

            // comparing value to other field (Greater than)
            $.validator.addMethod('ge', function (value, element, param) {
                return this.optional(element) || value > $(param).val();
            }, 'Invalid value');

            // comparing value to other field (Less than)
            $.validator.addMethod('le', function (value, element, param) {
                return this.optional(element) || value < $(param).val();
            }, 'Invalid value');

            // Currency Validation
            //$.validator.addMethod('currency', function (value, element, param) {
            //    return this.optional(element) || /^\d+(?:\.\d{0,2})$/.test(value);
            //}, 'Invalid value');

            // 
            $.validator.addMethod('currency', function (value, element, param) {
                return this.optional(element) || /^-?(?:\d+|\d{1,3}(?:[\s\.,]\d{3})+)(?:[\.,]\d+)?$/.test(value);
            }, 'Invalid value');

            // verify positive number
            $.validator.addMethod('positiveNumber', function (value) {
                return Number(value) > 0;
            }, 'Enter  positive number.');

            // validate PAN  number
            $.validator.addMethod("pan", function (value, element) {
                if (value.lenght != 0)
                    return this.optional(element) || /^[a-zA-Z]{5}\d{4}[a-zA-Z]{1}$/.test(value);
            }, "Invalid Pan Number");

            // validate Phone  number
            $.validator.addMethod("phonenumber", function (value, element) {
                return this.optional(element) || /^[7-9]{1}[0-9]{9}$/.test(value);
            }, "Invalid Phone Number");

            // validate PIN  number
            $.validator.addMethod("pinnumber", function (value, element) {
                return this.optional(element) || /^[5]{1}[0-9]{5}$/.test(value);
            }, "Invalid Pin Number");

            $.validator.addMethod("nowhitespace", function (value, element) {
                return this.optional(element) || /^\S+$/i.test(value);
            }, "No white space please");

            $.validator.addMethod("decimal", function (value, element) {
                return this.optional(element) || /^\d*\.?\d*$/.test(value);
            }, "");
            $.validator.addMethod("alphanum", function (value, element) {
                return this.optional(element) || /^[0-9a-zA-Z]+$/.test(value);
            }, "");
            $.validator.addMethod("time24", function (value, element) {
                if (!/^\d{2}:\d{2}$/.test(value)) return false;
                var parts = value.split(':');
                if (parts[0] > 23 || parts[1] > 59 || parts[2] > 59) return false;
                return true;
            }, "Invalid time format.");



            $("#resetall").click(function () {
                $('input[type=text]').each(function () {
                    $(this).val('');
                });
                $('input[type=password]').each(function () {
                    $(this).val('');
                });
                $("select").each(function () {
                    $(this).prop("selectedIndex", -1);
                });
                $('#lblproducttypedesc').hide();
                $('#lblproductgroupdesc').hide();
                $('#lblcontainerdesc').hide();
                $('#lblpackingdesc').hide();
                $('#lblstorageareadesc').hide();
                $('input[type=checkbox]').each(function () {
                    $(this).prop('checked', false);
                });
                $("#form").validate().resetForm();
            });

            $('input[type=text]').keyup(function (e) {
                var keycode = e.keyCode;
                if (keycode == 32) {
                    //alert(keycode);
                    if (this.value == " ") {
                        this.value = this.value.trim();
                    } else {
                        this.value = this.value.replace(/ +(?= )/g, '');
                    }
                }

            });
            $('textarea').keyup(function (e) {
                var keycode = e.keyCode;
                if (keycode == 32) {
                    //alert(keycode);
                    if (this.value == " ") {
                        this.value = this.value.trim();
                    } else {
                        this.value = this.value.replace(/ +(?= )/g, '');
                    }
                }

            });

            /* Notifications */

            $("#divnots").hide();
            $("#iconGlobe").click(function () {
                debugger;

                if ($("#divnots").is(":visible")) {
                    $("#divnots").hide();
                    $(".arw").hide();
                } else {
                    $("#divnots").show();
                    $(".notification-count").hide();
                }
            });
            if (typeof String.prototype.trim !== 'function') {
                String.prototype.trim = function () {
                    return this.replace(/^\s+|\s+$/g, '');
                }
            }
        });

        //  setInterval(GetNotifications, 1000 * 60 * 1);



    </script>
